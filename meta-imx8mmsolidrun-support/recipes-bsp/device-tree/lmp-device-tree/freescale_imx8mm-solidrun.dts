&lcdif {
	status = "okay";
};

&lcdif_to_dsim {
	remote-endpoint = <&mipi_dsi_in>;
};

&mipi_dsi {
	status = "okay";

	/delete-node/ port@0;
	ports {
		#address-cells = <1>;
		#size-cells = <0>;

		port@0 {
			reg = <0>;
			mipi_dsi_in: endpoint {
				remote-endpoint = <&lcdif_to_dsim>;
			};
		};

		port@1 {
			reg = <1>;
			mipi_dsi_out: endpoint {
				remote-endpoint = <&adv_bridge_in>;
				attach-bridge;
			};
		};
	};
};

&vpu_g1 {
