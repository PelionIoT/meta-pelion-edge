[Unit]
Description=Tunneling Proxy for gateways
Wants=wait-for-pelion-identity.service
After=wait-for-pelion-identity.service

[Service]
Restart=always
RestartSec=5
ExecCondition=sh -c '[ -f EDGE_DATA/edge_gw_config/identity.json ]'
ExecStart=EDGE_BIN/launch-edge-proxy.sh

[Install]
WantedBy=network.target
